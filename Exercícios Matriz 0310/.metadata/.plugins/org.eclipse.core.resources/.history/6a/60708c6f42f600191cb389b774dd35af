import java.util.Scanner;
public class MatrizBingo {
	public static void main(String[]args) {
		Scanner ler=new Scanner(System.in);
		int m[][]=new int[4][4];
		int i = 0, j, at, cont=0, k=0, x, win=0, help=0;
		int numero[]=new int[16];
		for(at=0;at<numero.length;at++){
			int rand=(int)((Math.random()*74)+1);
			boolean e = false;
			for(int grr=0;grr<at;grr++){
				if(numero[grr]==rand){
					e = true;
					break;
				}
				else{
					e = false;
				}
			} 
			if(!e){
				numero[at]=rand; 
			}
			else{
				at--;
			}
        }
		for(i=0, help=0; i<=3; i++) {
			for(j=0; j<=3; j++) {
				m[i][j]=numero[help];
				help++;
			}
		}
		for(i=0; i<=3; i++) {
			for(j=0; j<=3; j++) {
				System.out.print("[ "+m[i][j]+" ]");
				if(j==3) {
					System.out.print("\n");
				}
			}
		}
		System.out.println(" ");
		while(k==0) {
			System.out.println("Cante um número:");
			x=ler.nextInt();
			for(i=0; i<=3; i++) {
				for(j=0; j<=3; j++) {
					if(x==m[i][j]) {
						win++;
					}
				}
			}
			if(win==16) {
				k+=1;
			}
			else {
				k+=0;
			}
			cont++;
		}
		System.out.println("------");
		System.out.println("BINGO!");
		System.out.println("------");
		System.out.println("O jogador conseguiu no "+cont+"º número");
	}
}

